<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<AnchorPane prefHeight="500" prefWidth="700"
	xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controller.UserHomeController">
	<children>
		<ScrollPane fx:id="scrollPane" layoutX="14.0" layoutY="14.0" prefHeight="472.0" prefWidth="436.0">
         <content>
            <TilePane vgap="15" hgap="10" layoutX="24.0" layoutY="24.0" prefHeight="452.0" prefWidth="416.0" fx:id="tilePane">
				<padding>
            		<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         		</padding>
			</TilePane>
         </content>
      </ScrollPane>
		
		<Button layoutX="500" layoutY="90" prefHeight="27.0" prefWidth="100.0" mnemonicParsing="false" text="Create album" onAction="#createAlbum" fx:id="add" />
        <Button layoutX="500" layoutY="130" prefHeight="27.0" prefWidth="100.0" mnemonicParsing="false" text="Delete album" onAction="#deleteAlbum" fx:id="delete" />
        <Button layoutX="500" layoutY="170" prefHeight="27.0" prefWidth="100.0" mnemonicParsing="false" text="Rename" onAction="#renameAlbum" fx:id="rename" />
        <Button layoutX="500" layoutY="210" prefHeight="27.0" prefWidth="100.0" mnemonicParsing="false" text="Open" onAction="#openAlbum" fx:id="open" />
        <Button layoutX="500" layoutY="250" prefHeight="27.0" prefWidth="100.0" mnemonicParsing="false" text="Search Photos" onAction="#searchPhotos" fx:id="search" />
        <Button layoutX="586" layoutY="14.0" prefHeight="27.0" prefWidth="100.0" mnemonicParsing="false" text="Logout" onAction="#logout" fx:id="logout" />
	</children>
</AnchorPane>