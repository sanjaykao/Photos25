<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<AnchorPane prefHeight="278" prefWidth="500"
	xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controller.AdminHomeController">
	<children>
    	<ListView layoutX="14.0" layoutY="14.0" prefHeight="250.0" prefWidth="200.0" fx:id="listView">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      	</ListView>
      	<Button layoutX="250" layoutY="90" prefHeight="27.0" prefWidth="80.0" mnemonicParsing="false" text="Add user" onAction="#addUser" fx:id="add" />
        <Button layoutX="250" layoutY="130" prefHeight="27.0" prefWidth="80.0" mnemonicParsing="false" text="Delete user" onAction="#deleteUser" fx:id="delete" />
        <Button layoutX="406" layoutY="14.0" prefHeight="27.0" prefWidth="80.0" mnemonicParsing="false" text="Logout" onAction="#logout" fx:id="logout" />
	</children>
</AnchorPane>